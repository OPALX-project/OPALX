&NEWRUN
  Head='RF Photoinjector for PSI-XFEL Project - 150 MeV with QMs and 200 pC'
  RUN=4,
  Distribution = 'synthetic.ini',
  Xoff=0.0, Yoff=0.0
  Qbunch=0.200  ! To be changed for Q
  Lmagnetized=.F
  Lsub_cor=.T
  EmitS=.T
  PhaseS=.T
  TrackS=.T
  RefS=.T
  TcheckS=.F
  CathodeS=.F
  Local_emit=.T
  High_res=.T.
  TRACK_ALL=.T, PHASE_SCAN=.F,  AUTO_PHASE=.T
  ZSTART=0.0,   ZSTOP=3.11   ! m downstream from cathode
  Zemit=1000
  Zphase=1000
  Lmonitor=.T
  check_ref_part=.F
  H_max=0.001
  H_min=0.000
  LSUB_COR =.T
  Debunch=0.0
 /

 &SCAN
