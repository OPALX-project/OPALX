@article{1995CastilloHymanShashkovSteinbergB,
	Author = {Jose Castillo and James M. Hyman and Mikhail J. Shashkov and Stanly Steinberg},
	Coden = {CMAPDK},
	Fjournal = {Computers \& Mathematics with Applications. An International Journal},
