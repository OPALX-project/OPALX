@article{1986KreissManteuffelSwartzWendroffWhite,
	Author = {H.-O. Kreiss and T. A. Manteuffel and B. Swartz and B. Wendroff and A. B. White, Jr.},
	Coden = {MCMPAF},
	Fjournal = {Mathematics of Computation},
	Issn = {0025-5718},
	Journal = {Math. Comp.},
	Mrclass = {65L05 (40A30 65D25 65L10)},
	Mrnumber = {88b:65082},
	Mrreviewer = {Randall J. LeVeque},
	Note = {{\em NOTES:} From the review: ""Supra-convergent" finite difference schemes are those for which the solution error has a higher order of accuracy than is to be expected from the local truncation error. For example, standard methods that are second order accurate on regular grids may have a truncation error that reduces to first order on irregular grids. Nonetheless, some such methods exhibit second order accuracy globally. This paper explores this phenomenon and gives some conditions under which second order accuracy can be expected. It is a companion paper to one by Manteuffel and White [same journal 47 (1986), no. 176, 511--535; MR 87m:65116] and relates some of the results and methods of proof to analogues in the finite element theory, where the corresponding phenomenon is known as superconvergence. References are given to earlier work in this direction, notably by A. N. Tikhonov and A. A. Samarski\u{\i} [Zh. Vychisl. Mat. i Mat. Fiz. 2 (1962), 812--832; MR 29 \#5392]."},
	Number = {176},
	Pages = {537--554},
	Title = {Supra-convergent schemes on irregular grids},
	Volume = {47},
	Year = {1986}}

@article{1986Nedelec,
	Author = {J.-C. N{\'e}d{\'e}lec},
	Coden = {NUMMA7},
	Fjournal = {Numerische Mathematik},
	Issn = {0029-599X},
	Journal = {Numer. Math.},
	Mrclass = {65N30},
	Mrnumber = {88e:65145},
	Mrreviewer = {Reinhard Scholz},
	Note = {{\em NOTES:} Recommended. From the review: "In this paper two new families of mixed finite elements in $\mathbf R^3$ are introduced which are conforming in $H(\mathrm{div})$ and $H(\mathrm{curl})$, respectively. In both cases the decomposition of the domain into tetrahedra, cubes and prisms is considered. These elements are later used to get approximate solutions of the Stokes equations. This paper may be considered as a continuation of an earlier paper of the author [same journal 35 (1980), no. 3, 315--341; MR 81k:65125]. Some other results in the same direction have recently been obtained by F. Brezzi, J. Douglas, Jr., R. Dur\'{a}n and M. Fortin [ibid. 51 (1987), no. 2, 237--250]."},
