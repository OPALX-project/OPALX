@article{2001HiptmairB,
	Author = {R. Hiptmair},
	Coden = {NUMMA7},
	Fjournal = {Numerische Mathematik},
	Issn = {0029-599X},
	Journal = {Numer. Math.},
	Mrclass = {39A05 (35J25 35K57 39A12 65N06)},
	Mrnumber = {1 872 728},
	Mrreviewer = {Thomas H. Sonar},
	Note = {{\em NOTES: }Recommended. From the review: "Many partial differential equations of mathematical physics are equations concerning exterior derivatives in the language of differential forms. Although many authors noted this before, Hiptmair not only aims at a unified description of several different physical phenomena but also outlines a general approach which leads to practical hints for the construction of finite element and finite volume approximations for these equations. The author starts from a generic discrete Hodge operator and shows that many FEMs as well as finite volume methods appear to be specializations of this operator. Ideas for a unified convergence analysis in the framework of discrete exterior calculus are outlined."},
	Number = {2},
	Pages = {265--289},
	Title = {Discrete {H}odge operators},
	Volume = {90},
	Year = {2001}}

