@article{2001BerndtLipnikovMoultonShashkov,
	Author = {Markus Berndt and Konstantin Lipnikov and J. David Moulton and Mikhail J. Shashkov},
	Fjournal = {East-West Journal of Numerical Mathematics},
	Issn = {0928-0200},
	Journal = {East-West J. Numer. Math.},
	Mrclass = {65N06 (65N30)},
	Mrnumber = {2003a:65085},
	Mrreviewer = {Hans-Peter Helfrich},
	Note = {{\em NOTES: }From the review: "The authors consider mimetic discretizations of the linear first-order system that describes stationary diffusion. Mimetic methods are those that preserve discrete analogues of fundamental integral identities, such as the divergence theorem. In this article the support operators method is utilized, where an initial discrete approximation for a first-order operator is employed to construct a discretization for another operator. This is done by a discrete formulation of an integral identity with the aid of a discrete scalar product. For example, an approximation of the divergence supports an approximation of the gradient, by Gauss's Theorem. In this paper, convergence of the support operator discretization for linear diffusion is shown by developing a connection of this mimetic discretization with mixed finite element methods on triangular and quadrilateral grids. The convergence properties are verified by numerical examples."},
	Number = {4},
	Pages = {265--284},
	Title = {Convergence of mimetic finite difference discretizations of the diffusion equation},
	Volume = {9},
	Year = {2001}}

