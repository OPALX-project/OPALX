@article{2001HymanShashkovSteinberg,
	Author = {James M. Hyman and Mikhail J. Shashkov and Stanly Steinberg},
	Coden = {CMAPDK},
	Fjournal = {Computers \& Mathematics with Applications. An International Journal},
	Issn = {0898-1221},
	Journal = {Comput. Math. Appl.},
	Mrclass = {65N06},
	Mrnumber = {2002k:65172},
	Mrreviewer = {John C. Strikwerda},
	Note = {{\em NOTES:} From the review: "The authors discuss the importance of the choice of inner product on the accuracy of the solution for mimetic finite difference methods. Mimetic methods are those that satisfy discrete analogs of differential relationships such as the divergence theorem. For a mimetic method the definition of the discrete gradient depends on the definition of the discrete divergence operator. The authors use the support-operators method to discretize differential equations. It is shown that the proper choice of inner product for scalar functions and vector fields can have a significant effect on the accuracy of the numerical methods. Numerical examples are presented illustrating the improvement due to proper choice of the inner product"},
	Number = {12},
	Pages = {1527--1547},
	Title = {The effect of inner products for discrete vector fields on the accuracy of mimetic finite difference methods},
	Volume = {42},
	Year = {2001}}

