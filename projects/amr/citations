@article{1987BrezziDouglasDuranFortin,
	Author = {Franco Brezzi and Jim {Douglas, Jr.}, and Ricardo Dur{\'a}n and Michel Fortin},
	Coden = {NUMMA7},
	Fjournal = {Numerische Mathematik},
	Issn = {0029-599X},
	Journal = {Numer. Math.},
	Mrclass = {65N30},
	Mrnumber = {88f:65190},
	Mrreviewer = {Jaroslaw Jelen},
	Note = {{\em NOTES:} Highly recommended. From the review: "This paper examines both theoretical and computational aspects of mixed finite element methods for solving second-order boundary value problems for elliptic equations. New simplicial and cubic elements are presented, and an error analysis is given for the spaces formed from combinations of these two kinds of elements. Moreover, a new approximation $u_h^*$ of $u$ is constructed to solve the Dirichlet problem, $-{\rm div}(a(x)\mathrm{grad}\,u)=f$ for $x\in G$, $u=-g$ for $x\in\partial G$, by hybridization of the mixed method. Albeit a complete spectral analysis is not carried out for the new alternating-direction process introduced by the authors, based on the Arrow-Hurwitz iterative technique, this process is of great value for the numerical application of the proposed method."},
	Number = {2},
	Pages = {237--250},
	Title = {Mixed finite elements for second order elliptic problems in three variables},
	Volume = {51},
	Year = {1987}}

@book{1987Heinrich,
	Address = {Basel},
	Author = {Bernd Heinrich},
	Isbn = {3-7643-1880-5},
	Mrclass = {65N06},
	Mrnumber = {91d:65145},
	Mrreviewer = {M. Zl{\'a}mal},
	Note = {A generalized approach to second order elliptic problems. {\em NOTES:} From the review: "This monograph is devoted to the analysis and justification of the balance method, often called the box or finite control volume method. With the exception of the last chapter the author considers the equation (1) $Lu=f(x)$, $x=(x_1, x_2)\in \Omega\subset \mathbf R^2$, with mixed boundary conditions where $Lu$ is a formally selfadjoint elliptic operator. The boundary value problem is briefly written in the form (2) $Au=F$. Irregular grids formed by triangles, rectangles and the combination of these elements are described in great detail. In addition, secondary grids are introduced by defining boxes via the perpendicular bisectors or via the medians. The balance method starts with identities called balance equations. For instance, in the case of the Poisson equation $-\Delta u=f(x)$, by using the Gauss formula we obtain (3) $-\int_{\partial \kappa} (\partial u/\partial n)\,ds=\int_\kappa f\,dx$ for all $\kappa \subset \overline{\Omega}$. Discretizing (3), where $\kappa$ is a box, by means of suitable quadrature and finite difference formulas we obtain the discrete operator $A_h$ and the discrete right-hand side $F_h$, and the discrete solution $y$ is defined by $A_h y=F_h$. Here $A_h$ and $F_h$ are constructed for the case that the operator $Lu$ is of the form $(4) Lu=-\sum^2_{i=1} (\partial/\partial x_i)(k\partial u/\partial x_i) +cu.$ The author studies the properties of $A_h$ and the corresponding matrices, especially with respect to monotonicity ($M$-matrices), symmetry and positive definiteness. Discrete analogues of Sobolev norms as well as the $C$-norm are introduced. A trace theorem, discrete analogues of the Green formula and of the Friedrichs-Poincar\'{e}-type inequalities are derived. Further, estimates of $A_h$ in different norms are given. An approach is developed to get error estimates and convergence proofs with discrete analogues of the $W^2_1$- and $C$-norms under the assumption that $u\in W^k_2(\Omega)$, $k\ge 2$. In the last short chapter the author applies the balance method to the solution of the diffusion-convection equation using upwind approximation with respect to the coordinate axes."},
	Pages = {206},
	Publisher = {Birkh\"auser Verlag},
	Series = {Internationale Schriftenreihe zur Numerischen Mathematik [International Series of Numerical Mathematics]},
	Title = {Finite difference methods on irregular networks},
	Volume = {82},
	Year = {1987}}

