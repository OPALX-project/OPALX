	Mrnumber = {98i:39004},
	Mrreviewer = {Philip Huddleston},
	Note = {Recommended. Support Operator Method meets natural projections. From the review: "The authors initiate a series of papers on finite-difference methods where the discretization of vector and tensor calculus is based on the support operator method [A. A. Samarski\u{\i} et al., Differentsialc nye Uravneniya 18 (1982), no. 7, 1251--1256, 1287; MR 83k:65072]. In this paper, the first steps in developing a set of mimetic difference operators are taken. Discrete analogues of the first-order operators divergence, gradient, and curl, are presented, based on the usual coordinate-free integral operator definitions of these operators. These analogues are the reference or support operators of the method. Discrete analogues of various familiar theorems are proven, e.g., Gauss' theorem. An alternate approach to mimetic finite difference methods based on concepts from algebraic topology is summarized in a book by A. A. Dezin [Multidimensional analysis and discrete models, Translated from the Russian by Irene Aleksanova, CRC, Boca Raton, FL, 1995; MR 98b:00015]."},
	Number = {4},
	Pages = {81--104},
	Title = {Natural discretizations for the divergence, gradient, and curl on logically rectangular grids},
	Volume = {33},
	Year = {1997}}

@article{1997HymanShashkovB,
	Author = {James M. Hyman and Mikhail J. Shashkov},
	Coden = {ANMAEL},
	Fjournal = {Applied Numerical Mathematics. An IMACS Journal},
	Issn = {0168-9274},
	Journal = {Appl. Numer. Math.},
	Mrclass = {39A12 (65C20 65N06)},
	Mrnumber = {99c:39018},
	Mrreviewer = {Sui Yang Chen},
	Note = {Support Operator Method. From the review: "The authors use the support-operator method to derive new discrete approximations of the divergence, gradient and curl using discrete analogs of the integral identities satisfied by the differential operators. They create a discrete analog of vector analysis on logically rectangular, nonorthogonal, nonsmooth grids. By constructing the adjoints to the natural discrete operators, the authors create additional discrete first-order operators with complementary domains and ranges. The new operators are defined to be compatible with their original discrete operators. They show that the operators satisfy discrete analogs of the major theorems of vector analysis relating the differential operators. This paper is the second in a series of papers by the authors; it is an extension of [Comput. Math. Appl. 33 (1997), no. 4, 81--104; MR 98i:39004]."},
	Number = {4},
	Pages = {413--442},
	Title = {Adjoint operators for the natural discretizations of the divergence, gradient and curl on logically rectangular grids},
