@article{2001CastilloHymanShashkovSteinberg,
	Author = {Jos\'{e} Castillo and James M. Hyman and Mikhail J. Shashkov and Stanly Steinberg},
	Coden = {ANMAEL},
	Fjournal = {Applied Numerical Mathematics. An IMACS Journal},
	Issn = {0168-9274},
	Journal = {Appl. Numer. Math.},
	Mrclass = {65D25},
	Mrnumber = {2002f:65030},
	Mrreviewer = {Mu Sheng Wei},
	Note = {{\em NOTES:} From the review: "This paper derives fourth- and sixth-order finite difference approximations for the first derivative of a differentiable function on a bounded interval. The authors first express the derivative of a function at any nodal point as a linear combination of the function at all nodal points with parameters to be determined. They then apply the discrete divergence theorem of the product of two differentiable functions in 1D (integration by parts), so that when the functions are polynomials of degrees not greater than $k=4$ or $ 6$, this formula becomes an identity."},
	Number = {1-2},
	Pages = {171--187},
	Title = {Fourth- and sixth-order conservative finite difference approximations of the divergence and gradient},
	Volume = {37},
	Year = {2001}}

