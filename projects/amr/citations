@article{1976Tonti,
	Author = {Enzo Tonti},
	Journal = {Rend. Sem. Mat. Fis. Milano},
	Mrclass = {83.58 (02G99)},
	Mrnumber = {58 \#14974},
	Mrreviewer = {George S. Whiston},
	Note = {{\em NOTES:} Not a numerical paper, but contains important material. Author's summary: "Starting from the observation that among the physical quantities of every physical theory there are many that are naturally referred to the basic geometric and chronometric objects, it is shown how one may obtain a classification scheme for the physical quantities of every physical theory. This permits us to assign a definite behaviour of physical quantities under time reversal and space inversion. The association of physical quantities with chrono-geometric objects gives a motivation for the introduction in physical theories of the notions of multivectors, fiber bundles, exterior differential forms and connection theory. Typical equations of physics are shown to be described by a single mathematical process, the coboundary process (exterior differential). The basic equations of physics are then divided into two classes: metric independent equations, i.e., purely topological equations, and metric dependent equations, the phenomenological equations. Classification schemes for the main physical theories conclude the paper."},
	Pages = {163--257 (1978)},
	Title = {Sulla struttura formale delle teorie fisiche},
	Volume = {46},
	Year = {1976}}

@article{1977TontiB,
	Author = {Enzo Tonti},
	Journal = {Appl. Math. Modelling},
	Mrclass = {02A05 (00A25)},
	Mrnumber = {58 \#5001},
	Note = {{\em NOTES:} Not a numerical paper, but contains important material. Author's summary: "An investigation of the reason for analogies between physical theories is carried out. The reason if found in the fact that in every physical theory there are basic physical quantities which are referred to geometric and chronometric elements. The analysis is pursued further using the rudiments of algebraic topology, i.e, the notion of `chain' and that of `coboundary'. A rational classification scheme for the physical variables and equations of an arbitrary physical theory is then constructed. Some examples of these schemes are given."},
	Number = {1},
	Pages = {37--50},
	Title = {The reason for analogies between physical theories},
	Volume = {1},
	Year = {1976/77}}

