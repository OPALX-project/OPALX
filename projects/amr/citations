@article{1982SamarskiiTishkinFavorskiiShashkov,
	Author = {A. A. Samarski\u{\i} and V. F. Tishkin and A. P. Favorski\u{\i} and M. Yu. Shashkov},
	Fjournal = {Differentsial nye Uravneniya},
	Issn = {0374-0641},
	Journal = {Differentsial nye Uravneniya},
	Mrclass = {65N05},
	Mrnumber = {83k:65072},
	Mrreviewer = {Ra\u{\i} cho Lazarov},
	Note = {English translation: Differential Equations 18 (1982), no. 7, 881--885. {\em NOTES:} Support Operator Method. From the review: "In a previous paper [Dokl. Akad. Nauk SSSR 258 (1981), no. 5, 1092--1096] the authors constructed a compatible system of difference approximations for the basic operators of vector analysis. These difference operators have the main properties of the corresponding differential operators such as $$ \oint_S\varphi(\vec A,\vec n)\,dS=\int_V\varphi\mbox{div}\vec A\,dV+\int_V(\vec A,\mbox{grad}\,\varphi)\,dV. \mbox{\bf (1)}$$ In the present paper they extend their investigations to the operators of tensor analysis. The construction of the operators is made in the following way. Directly on the mesh, they approximate one of the operators div, grad or rot and consider it as a determining difference operator. Under conditions of compatibility, similar to (1) but for tensor objects, the finite difference approximations of the other operators are derived. The technique developed is illustrated on the dynamic equations of continuous media for elastic-plastic flows. This novel method is perfectly suitable for numerical solution of the multidimensional, nonlinear problems of mathematical physics and may have a wide range of applications."},
	Number = 7,
	Pages = {1251--1256, 1287},
	Title = {Use of the support operator method for constructing difference analogues of operations of tensor analysis},
	Volume = 18,
	Year = 1982}

